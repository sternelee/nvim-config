{
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.messageLevel": "warning",
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.source.file.triggerCharacters": ["/"],
  "signature.enable": true,
  "semanticTokens.enable": false,
  "semanticTokens.incrementTypes": [
    "variable",
    "string",
    "parameter",
    "class",
    "enum",
    "interface",
    "namespace",
    "type",
    "typeParameter",
    "parameter",
    "enumMember",
    "property",
    "function",
    "method"
  ],
  "semanticTokens.combinedModifiers": [
    "deprecated",
    "declaration",
    "static",
    "async",
    "readonly",
    "defaultLibrary",
    "local"
  ],
  "codeLens.enable": true,
  "codeLens.separator": "",
  "codeLens.subseparator": " │ ",
  "codeLens.position": "eol",
  "suggest.enablePreselect": true,
  "suggest.noselect": true,
  "suggest.virtualText": true,
  "suggest.defaultSortMethod": "length",
  "suggest.snippetIndicator": "",
  "suggest.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder",
    "highlight": "Normal",
    "maxWidth": 120
  },
  "suggest.languageSourcePriority": 99,
  "suggest.triggerCompletionWait": 50,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.completionItemKindLabels": {
    "default": "",
    "text": "",
    "method": "",
    "function": "ƒ",
    "constructor": "",
    "field": "料",
    "variable": "",
    "class": "",
    "interface": "",
    "module": "",
    "property": "",
    "unit": "",
    "value": "",
    "enum": "",
    "keyword": "",
    "snippet": "",
    "color": "",
    "file": "",
    "reference": "渚",
    "folder": "",
    "enumMember": "",
    "constant": "",
    "struct": "פּ",
    "event": "鬒",
    "operator": "Ψ",
    "typeParameter": ""
  },
  "diagnostic.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder",
    "highlight": "Normal",
    "maxWidth": 120,
    "focusable": false
  },
  "diagnostic.signPriority": 20,
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.locationlistUpdate": true,
  "diagnostic.virtualText": false,
  "git.addGBlameToVirtualText": true,
  "hover.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder",
    "highlight": "Normal",
    "maxWidth": 120,
    "focusable": false
  },
  "signature.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder",
    "highlight": "Normal",
    "maxWidth": 120,
    "focusable": false
  },
  "dialog.floatHighlight": "Normal",
  "dialog.floatBorderHighlight": "Normal",
  "html.trace.server": "off",
  "css.trace.server": "off",
  "highlight.colors.enable": false,
  "tsserver.trace.server": "off",
  "tsserver.ignoreLocalTsserver": true,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.format.semicolons": "remove",
  "typescript.format.semicolons": "remove",
  "eslint.packageManager": "pnpm",
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": true,
  "json.trace.server": "off",
  "json.schemas": [
    {
      "fileMatch": ["*/nvim/snippets/*.json", "!*/nvim/snippets/snippets.json"],
      "url": "https://raw.githubusercontent.com/kevinhwang91/dotfiles/main/nvim/snippets/snippets.json"
    }
  ],
  "outline.keepWindow": true,
  "outline.sortBy": "position",
  "outline.splitCommand": "botright 40vs",
  "outline.showLineNumber": true,
  "cSpell.enabled": false,
  "cSpell.ignoreWords": [
    "xunlei",
    "nvim",
    "XLJS",
    "macintel",
    "Xbase",
    "Jsbridge",
    "KHTML",
    "SHOUJIXUNLEI",
    "botright",
    "vetur",
    "Quickfix",
    "subseparator",
    "noselect",
    "struct",
    "borderhighlight",
    "focusable",
    "locationlist",
    "Bolb",
    "Bybl",
    "Neovim",
    "Vite",
    "Vitesse",
    "animejs",
    "athing",
    "blueimp",
    "callapp",
    "comlink",
    "contant",
    "gcid",
    "iconfont",
    "lazyload",
    "nuxt",
    "nuxtjs",
    "pinia",
    "pnpm",
    "prettytable",
    "printstd",
    "qrcode",
    "reqwest",
    "sget",
    "sternelee",
    "storylink",
    "swiper",
    "thunderx",
    "unplugin",
    "userid",
    "vitejs",
    "vueunused",
    "windi",
    "xlpan",
    "xlpfont",
    "xunleiapp",
    "yunpan"
  ],
  "Lua.telemetry.enable": false,
  "Lua.diagnostics.globals": ["vim", "use", "bit"],
  "Lua.hint.enable": false,
  "languageserver": {},
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "blade",
    "htmldjango",
    "typescriptreact",
    "javascriptreact"
  ]
}
