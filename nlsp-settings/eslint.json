{
  "autoFixOnSave": true,
  "eslint.codeAction.disableRuleComment": {
    "commentStyle": "line",
    "enable": true,
    "location": "separateLine"
  },
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.format.enable": true,
  "eslint.onIgnoredFiles": "off",
  "eslint.packageManager": "npm",
  "eslint.quiet": false,
  "eslint.run": "onType"
}
